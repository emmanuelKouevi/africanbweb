{"ast":null,"code":"import axios from 'axios';\nimport $ from 'jquery';\nimport { API_RECUPERER_LISTE_OFFRE_VOYAGE } from '../globalConfig/globalConstConfig';\nexport default {\n  name: \"SelectionnerOffreVoayge\",\n  data() {\n    return {\n      errorMsg: null,\n      loading: true,\n      search: '',\n      headers: [],\n      offreVoyageDisponibleList: [],\n      offreVoyageObject: {\n        data: {\n          compagnieTransportRaisonSociale: \"COMPAGNIE KOUEVI CT\"\n        }\n      }\n    };\n  },\n  methods: {\n    //OBTENIR LISTE DES OFFRES DE VOYAGES DISPONIBLES PAR COMPAGNIE\n    async obtenirOffreVoyageListParCompagnie() {\n      this.loading = false;\n      axios.post(API_RECUPERER_LISTE_OFFRE_VOYAGE, this.offreVoyageObject).then(response => {\n        console.log(response);\n        if (response.status == 200) {\n          if (response.data.status.code != 800) {\n            this.errorMsg = response.data.status.message;\n            $(\".alert-error\").fadeIn();\n            setTimeout(function () {\n              $(\".alert-error\").fadeOut();\n            }, 4000);\n          } else {\n            this.offreVoyageDisponibleList = response.data.items;\n          }\n        } else {\n          this.errorMsg = \"Erreur\";\n          $(\".alert-error\").fadeIn();\n          setTimeout(function () {\n            $(\".alert-error\").fadeOut();\n          }, 4000);\n        }\n      }).catch(e => {\n        this.errorMsg = e;\n      });\n    }\n  },\n  //EDITER UNE OFFRE DE VOYAGE\n  editerOffreVoyage() {},\n  mounted() {\n    this.obtenirOffreVoyageListParCompagnie();\n  }\n};","map":{"version":3,"mappings":"AAiCA;AACA;AACA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC,WAEA;MAEAC;MAEAC;QACAN;UACAO;QACA;MACA;IACA;EACA;EAEAC;IACA;IACA;MACA;MACAC;QACAC;QACA;UACA;YACA;YACAC;YACAC;cACAD;YACA;UACA;YACA;UACA;QACA;UACA;UACAA;UACAC;YACAD;UACA;QACA;MACA;QACA;MACA;IACA;EACA;EAGA;EACAE;EAGAC;IACA;EACA;AACA","names":["name","data","errorMsg","loading","search","headers","offreVoyageDisponibleList","offreVoyageObject","compagnieTransportRaisonSociale","methods","axios","console","$","setTimeout","editerOffreVoyage","mounted"],"sourceRoot":"src/components/administration_compagnie_transport","sources":["SelectionnerOffreVoyage.vue"],"sourcesContent":["<template>\n    <v-app>\n        <v-card>\n            <v-card-title class=\"title-card\">LISTE DES OFFRES DE VOYAGES\n                <v-spacer></v-spacer>\n                <v-text-field v-model=\"search\" append-icon=\"mdi-magnify\" label=\"Search\" single-line hide-details></v-text-field>               \n            </v-card-title>\n            \n            <v-data-table\n                :headers=\"headers\"\n                :items=\"offreVoyageDisponibleList\"\n                :loading=\"loading\"\n                :search=\"search\">\n\n                <template v-slot:[`item.statusUtilActualDesignation`]=\"{ item }\">\n                    <v-chip small v-if=\"item.statusUtilActualDesignation == 'compagnieTransportEncoursTraitement'\" color=\"teal\" text-color=\"white\" class=\"mr-2\"><span class=\"etat font-weight-bold\">En cours de traitement</span></v-chip>\n                    <v-chip v-else color=\"blue\" text-color=\"white\" class=\"mr-2\"><span class=\"etat\">Aucune d√©cision</span></v-chip>\n                </template>\n\n                <template v-slot:[`item.actions`]=\"{ item }\">\n                    <v-icon title=\"editer\" color=\"blue\" small class=\"mr-2\" @click=\"editCompagnieTransport(item)\">mdi-pencil</v-icon>\n                    <v-icon title=\"valider\" color=\"success\" small class=\"mr-2\" @click=\"validerCompagnieTransport(item)\">mdi-sync</v-icon>                       \n                    <v-icon title=\"supprimer\" color=\"red\" small class=\"mr-2\" @click=\"supprimerProduitLogement(item)\">mdi-delete</v-icon>\n                </template>\n\n            </v-data-table>\n            <v-alert class=\"myalert alert-error\" type=\"error\" width=\"350px\" dismissible>{{ errorMsg }}</v-alert>\n        </v-card>\n    </v-app>\n</template>\n\n<script>\n\nimport axios from 'axios';\nimport $ from 'jquery';\nimport { API_RECUPERER_LISTE_OFFRE_VOYAGE } from '../globalConfig/globalConstConfig';\n\nexport default {\n    name:\"SelectionnerOffreVoayge\",\n    data(){\n        return{\n            errorMsg: null,\n            loading:true,\n            search:'',\n            headers:[\n                \n            ],\n\n            offreVoyageDisponibleList:[],\n\n            offreVoyageObject:{\n                data:{\n                    compagnieTransportRaisonSociale:\"COMPAGNIE KOUEVI CT\"\n                }\n            },\n        }\n    },\n\n    methods:{\n        //OBTENIR LISTE DES OFFRES DE VOYAGES DISPONIBLES PAR COMPAGNIE\n        async obtenirOffreVoyageListParCompagnie(){\n            this.loading = false;\n            axios.post(API_RECUPERER_LISTE_OFFRE_VOYAGE, this.offreVoyageObject ).then((response) => {\n                console.log(response)\n                if (response.status == 200) {\n                    if (response.data.status.code != 800) {\n                        this.errorMsg = response.data.status.message\n                        $(\".alert-error\").fadeIn();\n                        setTimeout(function(){\n                            $(\".alert-error\").fadeOut(); \n                        }, 4000)\n                    }else{\n                        this.offreVoyageDisponibleList = response.data.items\n                    }\n                }else{\n                    this.errorMsg = \"Erreur\";\n                    $(\".alert-error\").fadeIn();\n                    setTimeout(function(){\n                        $(\".alert-error\").fadeOut(); \n                    }, 4000)\n                }\n            }).catch((e) => {\n                this.errorMsg = e\n            })\n        }\n    },\n\n\n    //EDITER UNE OFFRE DE VOYAGE\n    editerOffreVoyage(){},\n\n\n    mounted(){\n        this.obtenirOffreVoyageListParCompagnie()\n    }\n}\n</script>\n\n<style scoped>\n    .title-card{\n        font-weight: bold;\n        color: black;\n        font-size: 17px;\n    }\n\n    .myalert{\n        display: none;\n        z-index: 1900;\n    }\n\n    .alert-error{\n        position: fixed;\n        top: 25px;\n        right:2%;\n        width: 25%;\n    }\n</style>"]},"metadata":{},"sourceType":"module"}